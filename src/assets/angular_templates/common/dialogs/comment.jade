md-dialog(aria-label="Folder")
  md-content
    .form-container
      h1 Comment
      fieldset
        field
          label User
          input(ng-model="user_name", type="text", disabled=true)
        field
          label Comment Body
          textarea(ng-model="description")

  .md-actions
    md-button.md-primary(ng-click="save()", md-no-ink="true") Save
    md-button(ng-click="cancel()", md-no-ink="true") Cancel