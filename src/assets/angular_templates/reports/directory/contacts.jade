h1.title Contacts List

section
  table
    tr
      th Last Name
      th First Name
      th Organization
      th Title
      th Email
    tr(ng-repeat="user in report.data.users")
      td {{ user.identity.last_name }} 
      td {{ user.identity.first_name }}
      td {{ user.organization || user.identity.organization }}
      td {{ user.identity.title }}
      td {{ user.identity.email }}
